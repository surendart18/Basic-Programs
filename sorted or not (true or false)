#include<bits/stdc++.h>
using namespace std;
int main()
{
	vector<int> num= {1,1,1,5,8,7};
	int n=num.size();
	int num_check=num[0];
	int flag=0;
	for(int i=1; i<n; i++)
	{
		if(num_check<=num[i])
		{
			num_check=num[i];
		}
		else
		{
		    flag=1;
            break;
		}
	}
	if(flag==0)  
	{
	    cout<<"True";
	}
	else   
	{
	    cout<<"False";
	}
	return 0;
}

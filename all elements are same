#include<bits/stdc++.h>
using namespace std;
int main()
{
    vector<int> num={1,1,1,1,1,1,1};
    int n=num.size();
    int flag=0;
    for(int i=1;i<n;i++)
    {
        if(num[0]==num[i])
        {
            flag=0;
        }
        else 
        {
            flag=1;
            break;
        }
    }
    if(flag==0)
    {
        cout<<"true";
    }
    else
    {
        cout<<"false";
    }
    return 0;
}

#include<bits/stdc++.h>
using namespace std;
int main()
{
    vector<int> num={1,2,3,4,5,6};
    int n=num.size();
    int tempIndex=n-1;
    for(int index=0;index<n;index++)
    {
        if(index<n/2)
        {
            cout<<num[index]<<" ";
        }
        else
        {
            cout<<num[tempIndex]<<" ";
            tempIndex--;
        }
    }
    return 0;
}

#include<bits/stdc++.h>
using namespace std;
int main()
{
    vector<int> num={5,7,8,4,1};
    int n=num.size();
    int small_num=INT_MAX;
    int large_num=INT_MIN;
    for(int i=0;i<n;i++)
    {
       if(num[i]<small_num)
       {
            small_num=num[i];
       }
       if(num[i]>large_num)
       {
           large_num=num[i];
       }
    }
    cout<<"smallest : "<<small_num<<endl;
     cout<<"largest : "<<large_num<<endl;
    return 0;
}

#include<bits/stdc++.h>
using namespace std;
int main()
{
    vector<int> num={1,6,2,4,7,9,2,7};
    int n=num.size();
    int check;
    cin>>check;
    int flag=0;
    for(int i=0;i<n;i++)
    {
        if(num[i]==check)
        {
            cout<<i<<" ";
            flag=1;
        }
    }
    if(flag==0)
    {
        cout<<-1;
    }
    return 0;
}
